<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="kmm" nsURI="http://kmm/1.0" nsPrefix="kmm">
  <eClassifiers xsi:type="ecore:EClass" name="Tile" eSuperTypes="CCore.ecore#//C2DFieldElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entity" eType="#//Entity"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="onCurrentPath" eType="ecore:EEnum CCore.ecore#//Direction"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="onOldPath" eType="ecore:EEnum CCore.ecore#//Direction"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="extra" eType="#//Entity"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="statsChanges" eType="#//StatsChanges"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Stats" eSuperTypes="#//StatsChanges">
    <eAnnotations source="http://cubemonstergames.com/UnityCMF">
      <details key="hasView" value="true"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="items" upperBound="3" eType="#//ItemSlot"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="quests" upperBound="-1"
        eType="#//QuestSlot" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Game">
    <eOperations name="move" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
      <eParameters name="tile" eType="#//Tile"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stats" lowerBound="1" eType="#//Stats"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="entities" upperBound="-1"
        eType="#//Entity" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="currentPath" upperBound="-1"
        eType="#//Move" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="tiles" upperBound="-1"
        eType="#//Tile" containment="true">
      <eAnnotations source="http://com.cubemonstergames/UnityCMF">
        <details key="2DField" value="9,11"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="currentTile" eType="#//Tile"
        volatile="true" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="oldPath" upperBound="-1"
        eType="#//Move" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="regularPool" eType="#//Pool"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="datePool" eType="#//Pool"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="seed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entity" eSuperTypes="#//Poolable">
    <eOperations name="apply"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="extraPool" eType="#//Pool"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Item" eSuperTypes="#//Entity">
    <eOperations name="applyPerTurn"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Door" eSuperTypes="#//Entity"/>
  <eClassifiers xsi:type="ecore:EClass" name="Move">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="actions" upperBound="-1"
        eType="ecore:EDataType CCore.ecore#//CAction"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="tile" eType="#//Tile"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Pool">
    <eOperations name="draw" eType="#//Poolable"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="poolables" upperBound="-1"
        eType="#//Poolable"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="QuestSlot">
    <eStructuralFeatures xsi:type="ecore:EReference" name="goal" eType="#//QuestGoal"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isAvailable" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isCompleted" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ItemSlot">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="turnsLeft" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="item" eType="#//Item"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="QuestGoal"/>
  <eClassifiers xsi:type="ecore:EClass" name="Poolable" abstract="true" interface="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="priority" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isUnique" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StatsChanges">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="kathisLove" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        defaultValueLiteral="1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="markusLove" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        defaultValueLiteral="1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="money" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="experience" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="level" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
</ecore:EPackage>
